{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [     
        {
            "name": "openvslam",
            "type": "cppdbg",
            "request": "launch",
            "program": "/openvslam/ros/devel/lib/openvslam/run_slam",
            "stopAtEntry": false,
            "cwd": "/openvslam/ros/src/openvslam/",
            "args": ["-v", "/vocab/orb_vocab.dbow2", "-c", "/vocab/blackfly.yaml", "--map-db", "/vocab/map.msg"],
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb"
            }
        },
    ]
}